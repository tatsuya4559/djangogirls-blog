{
  "compilerOptions": {
    "allowJs": true,
    "target": "es5",
    "lib": ["dom", "es2017"],
    "module": "umd",
    "jsx": "react",
    "noEmit": true,
    "sourceMap": true,
    "strict":true,
    "noFallthroughCasesInSwitch":true,
    "noImplicitReturns":true,
    "allowSyntheticDefaultImports": true,
    "esModuleInterop": true
  }
}
