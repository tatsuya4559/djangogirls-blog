{
  "compilerOptions": {
    "allowJs": true,
    "target": "es5",
    "lib": ["dom", "es5"],
    "module": "es2015",
    "jsx": "react",
    "sourceMap": true,
    "strict": true,
    "noFallthroughCasesInSwitch": true,
    "noImplicitReturns": true,
    "allowSyntheticDefaultImports": true,
    "esModuleInterop": true
  }
}
